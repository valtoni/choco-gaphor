<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>gaphor</id>
    <version>3.1.0</version>

    <title>gaphor (Install)</title>
    <summary>UML, SysML, RAAML, and C4 modeling application</summary>
    <description>
      Gaphor is a UML, SysML, RAAML, and C4 modeling application. It is designed to be easy to use, while still being powerful.
      Gaphor implements a fully-compliant UML 2 data model, so it is much more than a picture drawing tool.
    </description>

    <authors>Arjan Molenaar, Dan Yeaw</authors>
    <owners>Valtoni Boaventura</owners>

    <projectUrl>https://gaphor.org/</projectUrl>
    <projectSourceUrl>https://github.com/gaphor/gaphor</projectSourceUrl>
    <packageSourceUrl>https://github.com/valtoni/choco-gaphor</packageSourceUrl>
    <docsUrl>https://docs.gaphor.org/</docsUrl>
    <bugTrackerUrl>https://github.com/gaphor/gaphor/issues</bugTrackerUrl>
    <releaseNotes>https://github.com/gaphor/gaphor/releases/tag/3.1.0</releaseNotes>

    <iconUrl>https://gaphor.org/images/gaphor-24x24.png</iconUrl>
    <licenseUrl>https://github.com/gaphor/gaphor/blob/main/LICENSES/Apache-2.0.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <tags>gaphor uml sysml raaml c4 modeling diagram graphviz</tags>

    <dependencies>
      <dependency id="chocolatey-core.extension" version="[1.4.0]" />
      <dependency id="graphviz" />
    </dependencies>
  </metadata>

  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
